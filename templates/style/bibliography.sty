<| block style_bibliography |>
<| if use_bibliography |>

<| block style_bibliography_biblatex_style |>
\usepackage[backend=biber,style=alphabetic]{biblatex}
<| endblock |>

<| block style_bibliography_multicite |>
\renewcommand\multicitedelim{\addsemicolon\space}
<| endblock |>

<| block style_bibliography_resources |>
<| for resource in bibliography_resources |>
\addbibresource{<< resource >>}
<| endfor |>
<| endblock |>

<| block style_bibliography_translations |>

<| block style_bibliography_translations_chapter |>
<| if document_language == "lithuanian" |>
\defbibheading{mifheading}[\bibname]{\Chapter*{Šaltiniai}}
\newcommand{\Bibliography}{\printbibliography[heading=mifheading]}
<| else |>
\newcommand{\Bibliography}{\printbibliography}
<| endif |>
<| endblock |>

